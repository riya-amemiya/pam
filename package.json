{
  "name": "pam",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "rome format .",
    "dev": "WATCHPACK_POLLING=true prisma generate && next dev",
    "build": "prisma generate && prisma migrate deploy && yarn build:next",
    "build:next": "yarn lint && next build && yarn sitemap",
    "start": "next start",
    "sitemap": "next-sitemap",
    "lint": "eslint --fix src/ && rome check . && next lint && yarn markuplint && yarn typed-scss && tsc && rome format . --write",
    "lint:nofix": "eslint src/ && rome check . && next lint && yarn markuplint && yarn typed-scss && tsc && rome format .",
    "typed-scss": "typed-scss-modules \"src/**/*.{scss,css}\"",
    "typed-scss:watch": "typed-scss-modules \"src/**/*.{scss,css}\" --watch",
    "markuplint": "markuplint \"./src/**/*.{jsx,tsx}\"",
    "test": "jest",
    "deploy-test": "next build && yarn test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.15.0",
    "ai": "^2.1.3",
