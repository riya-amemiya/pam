    "million": "^2.4.6-beta.2",
    "next": "13.4.6",
    "next-auth": "^4.22.1",
    "next-seo": "^6.0.0",
    "openai-edge": "^1.0.0",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.7",
    "recoil": "^0.7.7",
    "recoil-persist": "^4.2.0",
    "remark-gemoji": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.32.1",
    "swr": "^2.1.5",
    "tailwind-variants": "^0.1.8",
    "the-new-css-reset": "^1.8.6",
    "ts-pattern": "^5.0.1",
    "umt": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@markuplint/jsx-parser": "^3.8.0",
    "@markuplint/react-spec": "^3.9.0",
