    "next-seo": "^6.1.0",
    "openai-edge": "^1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.7",
    "recoil": "^0.7.7",
    "recoil-persist": "^5.1.0",
    "remark-gemoji": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.32.4",
    "swr": "^2.2.0",
    "tailwind-variants": "^0.1.13",
    "the-new-css-reset": "^1.9.0",
    "ts-pattern": "^5.0.4",
    "umt": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
