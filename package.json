{
  "name": "pam",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "rome format .",
    "dev": "WATCHPACK_POLLING=true prisma generate && next dev",
    "build": "prisma generate && prisma db push && yarn lint && next build && yarn sitemap",
    "start": "next start",
    "sitemap": "next-sitemap",
    "lint": "eslint --fix src/ && rome check . && next lint && yarn markuplint && yarn typed-scss && tsc && rome format . --write",
    "make:page": "node scripts/makePage.js",
    "make:story": "node scripts/makeStory.js",
    "typed-scss": "typed-scss-modules \"src/**/*.{scss,css}\"",
    "typed-scss:watch": "typed-scss-modules \"src/**/*.{scss,css}\" --watch",
    "markuplint": "markuplint \"./src/**/*.{jsx,tsx}\"",
    "test": "jest",
    "deploy-test": "next build && yarn test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.13.0",
    "body-parser": "^1.20.2",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "next": "13.3.0",
    "next-auth": "^4.22.1",
    "next-seo": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.7",
    "recoil-persist": "^4.2.0",
    "rome": "^12.0.0",
    "sharp": "^0.32.0",
    "swr": "^2.1.2",
    "the-new-css-reset": "^1.8.4",
    "ts-pattern": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@markuplint/jsx-parser": "^3.6.1",
    "@markuplint/react-spec": "^3.6.1",
    "@storybook/addon-actions": "^7.0.4",
    "@storybook/addon-essentials": "^7.0.4",
    "@storybook/addon-interactions": "^7.0.4",
    "@storybook/addon-links": "^7.0.4",
    "@storybook/addon-mdx-gfm": "^7.0.4",
    "@storybook/addon-styling": "^1.0.0",
    "@storybook/addon-viewport": "^7.0.5",
    "@storybook/blocks": "^7.0.4",
    "@storybook/nextjs": "^7.0.4",
